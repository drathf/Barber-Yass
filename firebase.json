{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json",
    "database": "(default)",
    "location": "nam5"
  },
  "hosting": {
    "public": "dist", 
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html" 
      }
    ],
    "cleanUrls": true,
    "trailingSlash": false
  }
}
